        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Projekt2_WPF"
        mc:Ignorable="d"
        Title="WarningWindow" Height="146" Width="355.2">
    <Grid>
        <Label Content="Czy jesteś pewien?" Height="72" Margin="0,0,15.8,24" VerticalAlignment="Bottom" HorizontalContentAlignment="Center" FontSize="20" HorizontalAlignment="Right" Width="317"/>
        <Button x:Name="Yes" Content="Tak" HorizontalAlignment="Left" Margin="10,86,0,0" VerticalAlignment="Top" Width="114" Click="Yes_Click"/>
        <Button x:Name="No" Content="Nie" HorizontalAlignment="Left" Margin="222,88,0,0" VerticalAlignment="Top" Width="114" Click="No_Click"/>

    </Grid>
</Window>
